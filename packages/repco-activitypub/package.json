  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.5",
    "@urql/core": "^3.0.3",
    "brittle": "^2.4.0",
    "http-terminator": "^3.2.0",
    "nodemon": "^2.0.19"
  }
